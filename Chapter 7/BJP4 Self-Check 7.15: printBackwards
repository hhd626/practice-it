/**
*Write a method printBackwards that prints an array of integers in reverse order, in the following format.

* For the array {1, -3, 4, 7, 2}:

element [4] is 2
element [3] is 7
element [2] is 4
element [1] is -3
element [0] is 1
*/
import java.util.*;
public class ArraySwap {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n = input.nextInt();
        int[] list = new int[n];
        for ( int i = 0; i < n; i++) {
            list[i] = input.nextInt();
        }
        printBackwards(list);
    }
    
public static void printBackwards(int[] list) {
            if (list.length == 0) {
                System.out.println(" ");
            } else {
                for (int i = list.length - 1; i >= 0; i--) {
                    System.out.println("element [" + i + "] is " + list[i]);
                }
            }
}
